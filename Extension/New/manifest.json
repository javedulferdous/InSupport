{
    "manifest_version": 2,
    "name": "Quick filter Extension",
    "version": "1.0",
    "content_scripts": [
      {
        "matches":["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "permissions": [
      "tabs", "<all_urls>", "storage","activeTab"
  ],

  "browser_action": {
      "default_title":"Quick Filter",
      "default_popup": "basic.html",
      "default_icon":"icon.png"
    }
  }